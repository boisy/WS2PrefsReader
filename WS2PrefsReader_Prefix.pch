#ifdef __OBJC__
    #import <Cocoa/Cocoa.h>
#endif

#define NSDecodeObject(variable, key, decoder, className) { variable = [decoder decodeObjectForKey:key]; [variable retain]; if (variable == nil) variable = [className new]; }
#define NSEncodeObject(variable, key, encoder) { [encoder encodeObject:variable forKey:key]; }
